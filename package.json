{
  "name": "jssynth",
  "version": "1.2.0",
  "description": "A synthesizer and sequencer for your browser",
  "main": "jssynth.js",
  "repository": "https://github.com/jstrait/jssynth.git",
  "author": "Joel Strait <joel.strait@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-babel": "babel src/ --out-dir build",
    "build-webpack": "webpack --config webpack.config.js",
    "build-css": "sass sass/jssynth.scss | uglifycss > jssynth.css",
    "build": "yarn run build-css && yarn run build-babel && yarn run build-webpack"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "jest": "^22.3.0",
    "webpack": "^3.11.0"
  }
}
